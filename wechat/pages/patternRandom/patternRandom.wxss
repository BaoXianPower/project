/* pages/patternRandom/patternRandom.wxss */
.container {
  background-color: #fff;
  min-height: 100%;
  display: block;
}

.imageBox{
  width: 750rpx;
  height: 100vh;
  background-color: #fff;
}

.img-cus{
  width: 750rpx;
  height: 100vh;
  background-color: #fff;
}
.img-cus2{
  width: 180rpx;
  height: 180rpx;
  margin: 290rpx 0 0 285rpx;
}

.startBox{
  position: fixed;
  left: 150rpx;
  top: 340rpx;
  width: 470rpx;
  height: auto;  
}

.startBox .ico{
  width: 470rpx;
  height: 118rpx;
}

.startBox .btn-start{
   width: 430rpx;
  height: 110rpx;
  margin: 80rpx auto 0
}
.startBox .btn-start2{
   width: 430rpx;
  height: 110rpx;
  margin: 20rpx auto 0
}

.stopBox{
  position: fixed;
  left: 0;
  top: 510rpx;
  background-color: rgba(0,0,0,.7);
  width: 100%;
  height:90rpx;  
}

.stopBox .ico{
  width: 36rpx;
  height: 36rpx;
  margin: 27rpx 20rpx 0 270rpx;
}

.stopBox .text{
  width: auto;
  height: 90rpx;
  line-height: 90rpx;
  color: #fff;
}

.copyB {
    width: 750rpx;
    height: 100vh;
    background-color: #fff;
    position: relative;
}

.copyB .uplist-box {
    width: 100%;
    height: 650rpx;
    background-color: #F1F1F1;
    padding: 30rpx 0 0 30rpx;
}

.uplist-box .flex-box {
    justify-content: left;
}

.uplist-box .uplist-item {
    width: 212rpx;
    height: 283rpx;
    margin-right: 27rpx;
    margin-bottom: 30rpx;
    background-color: #D5D5D5;
}

.uplist-item-delete {
    width: 32rpx;
    height: 32rpx;
    position: absolute;
    top: 15rpx;
    right: 15rpx;
    z-index: 1;
}

.copyB .temp-box {
    width: 100%;
    height: 456rpx;
    background-color: #fff;
    padding-top: 30rpx;
}

.temp-box .temp-title {
    width: 100%;
    height: 33rpx;
}

.temp-box .temp-list {
    padding: 32rpx 0 0 0;
    /* 横向滚动必加条件 */
    width: 100%;
    overflow: hidden;
    white-space: nowrap;
}

.temp-list-item {
    display: inline-block;
    width: 150rpx;
    height: 226rpx;
    background-color: #D5D5D5;
    margin-right: 20rpx;
}

.temp-list-item:first-child {
    margin-left: 30rpx;
}

.temp-list-item-mask {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.3);
    text-align: center;
    padding-top: 85rpx;
}

.temp-list-item-mask .image-wrap {
    display: inline-block;
    width: 90rpx;
    height: 60rpx;
}

.temp-box .temp-start {
    width: 100%;
    padding: 26rpx 0 0 0;
    text-align: center;
}

.temp-start > view {
    display: inline-block;
    width: 320rpx;
    height: 80rpx;
    line-height: 80rpx;
    background-color: #000000;
    border-radius: 10rpx;
    color: #fff;
    font-size: 32rpx;
    text-align: center;
}

.copyB .temp-mask {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, .3);
    position: absolute;
    top: 0;
    left: 0;
    z-index: 10;
    text-align: center;
    padding-top: 405rpx;
}

.temp-mask .image-wrap {
    width: 152rpx;
    height: 152rpx;
    display: inline-block;
}

.temp-mask .text-memo {
    font-size: 34rpx;
    color: #fff;
}